{
	"name": "fab/formule",
	"type": "typo3-cms-extension",
	"description": "Render a variety of forms template based on the FE such as contact form, registration form, etc... effortless!",
	"homepage": "https://github.com/Ecodev/formule",
	"license": ["GPL-2.0-or-later"],
	"keywords": ["TYPO3 CMS", "form"],
	"support": {
		"issues": "https://github.com/Ecodev/formule/issues"
	},
	"autoload": {
		"psr-4": {
			"Fab\\Formule\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "formule"
		}
	},
	"require": {
		"typo3/cms-core": "^11.5",
		"michelf/php-markdown": "^1.9"
	}
}
