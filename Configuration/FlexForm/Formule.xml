<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:tab.general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.template>
						<TCEforms>
							<label>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:settings.template</label>
							<config>
								<type>select</type>
								<itemsProcFunc>Fab\Formule\Backend\TceForms->getTemplates</itemsProcFunc>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.template>
					<settings.redirectPid>
						<TCEforms>
							<label>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:settings.redirectPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.redirectPid>
					<settings.tableName>
						<TCEforms>
							<label>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:settings.tableName</label>
							<config>
								<type>input</type>
							</config>
						</TCEforms>
					</settings.tableName>
				</el>
			</ROOT>
		</general>
	</sheets>

</T3DataStructure>