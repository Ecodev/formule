<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:tab.general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.template>
						<TCEforms>
							<label>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:settings.template</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<itemsProcFunc>Fab\Formule\Backend\TceForms->getTemplates</itemsProcFunc>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</settings.template>
					<summary>
						<TCEforms>
							<label>LLL:EXT:formule/Resources/Private/Language/locallang.xlf:settings.summary</label>
							<config>
								<type>user</type>
								<userFunc>Fab\Formule\Backend\TceForms->renderSummary</userFunc>
							</config>
						</TCEforms>
					</summary>
				</el>
			</ROOT>
		</general>

